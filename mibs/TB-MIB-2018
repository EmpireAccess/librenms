                          connectingDatabase(4),
                          creatingDatabase(5),
                          updatingDatabase(6),
                          loadingDatabase(7),
                          startingServices(8),
                          applyingInitialConfiguration(9),
                          switchingNewConfiguration(100),
                          switchingNewPackage(101),
                          ready(102),
                          quitting(200)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
