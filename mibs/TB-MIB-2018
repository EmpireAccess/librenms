        INDEX { tbModuleIndex }
        ::= { tbHwFanTable 1 }

    TBHwFanEntry ::=
        SEQUENCE {
            tbHwFanName         DisplayString,
            tbHwFanCurrentSpeed DisplayString
    }

    tbHwFanName OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Fan name."
        ::= { tbHwFanEntry 1 }

    tbHwFanCurrentSpeed OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Current speed of a fan (tach units)."
          ::= { tbHwFanEntry 2 }

