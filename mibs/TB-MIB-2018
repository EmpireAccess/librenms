        ::= { tbHaNp1Entry 4 }

    tbHaNp1SecPatchPanelReady OBJECT-TYPE
        SYNTAX  INTEGER {
                          no(0),
                          yes(1)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
