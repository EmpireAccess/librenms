    -- ============================= --
    -- tb.tbMedia.tbHostFirewall.tbHostFirewallTable --
    -- ============================= --
    -- Host Firewall Table --
    tbHostFirewallTable OBJECT-TYPE
        SYNTAX  SEQUENCE OF TbHostFirewallEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "Lists Firewalls statistics information."
        ::= { tbHostFirewall 2 }

    tbHostFirewallEntry OBJECT-TYPE
        SYNTAX  TbHostFirewallEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "A Host Firewall table entry."
        INDEX   { tbModuleIndex }
        ::= { tbHostFirewallTable 1 }
        
    TbHostFirewallEntry ::=
        SEQUENCE {
                   tbHostFirewallName                       DisplayString,
                   tbHostFirewallState                      INTEGER,
                   tbHostFirewallSafeMode                   INTEGER,
                   tbHostFirewallSchedulingProblem          INTEGER,
                   tbHostFirewallDDOS                       INTEGER,
                   tbHostFirewallThresholdPassed            INTEGER,
                   tbHostFirewallHighCpu                    INTEGER,
                   tbHostFirewallStatDropEntryPct           Integer32,
                   tbHostFirewallStatAcceptEntryPct         Integer32,
                   tbHostFirewallStatFragmentEntryPct       Integer32,
                   tbHostFirewallStatRtpEntryPct            Integer32,
                   tbHostFirewallStatUnkVlan                Integer32,
                   tbHostFirewallStatUnkInterface           Integer32,
                   tbHostFirewallStatRxByte                 Integer32,
                   tbHostFirewallStatRxAcceptedByte         Integer32,
                   tbHostFirewallStatTxByte                 Integer32,
                   tbHostFirewallStatRxKbPerSec             Integer32,
                   tbHostFirewallStatRxAccKbPerSec          Integer32,
                   tbHostFirewallStatTxKbPerSec             Integer32,
                   tbHostFirewallStatRxPeakKbPerSec         Integer32,
                   tbHostFirewallStatRxAccPeakKbPerSec      Integer32,
                   tbHostFirewallStatTxPeakKbPerSec         Integer32,
                   tbHostFirewallStatRxPkt                  Integer32,
                   tbHostFirewallStatRxAccPkt               Integer32,
                   tbHostFirewallStatTxPkt                  Integer32,
                   tbHostFirewallStatRxPktPerSec            Integer32,
                   tbHostFirewallStatRxAccPktPerSec         Integer32,
                   tbHostFirewallStatTxPktPerSec            Integer32,
                   tbHostFirewallStatRxPeakPktPerSec        Integer32,
                   tbHostFirewallStatRxAccPeakPktPerSec     Integer32,
                   tbHostFirewallStatTxPeakPktPerSec        Integer32,
                   tbHostFirewallStatRxArpPkt               Integer32,
                   tbHostFirewallStatRxArpPktPerSec         Integer32,
                   tbHostFirewallStatRxArpPeakPktPerSec     Integer32,
                   tbHostFirewallStatRxDhcpPkt              Integer32,
                   tbHostFirewallStatRxDhcpPktPerSec        Integer32,
                   tbHostFirewallStatRxDhcpPeakPktPerSec    Integer32,
                   tbHostFirewallStatRxIcmpPkt              Integer32,
                   tbHostFirewallStatRxIcmpPktPerSec        Integer32,
                   tbHostFirewallStatRxIcmpPeakPktPerSec    Integer32,
                   tbHostFirewallStatRxL2BroadcastPkt       Integer32,
                   tbHostFirewallStatRxL2UnsupportedPkt     Integer32,
                   tbHostFirewallStatRxMulticastIpPkt       Integer32,
                   tbHostFirewallStatRxIpFragments          Integer32,
                   tbHostFirewallStatRxDroppedIpFragments   Integer32,
                   tbHostFirewallStatRxInvalidIpv4Size      Integer32,
                   tbHostFirewallStatRtpRxPkt               Integer32,
                   tbHostFirewallStatRtpTxPkt               Integer32,
                   tbHostFirewallStatRtpRxVoicePkt          Integer32,
                   tbHostFirewallStatRtpTxVoicePkt          Integer32,
                   tbHostFirewallStatRtpRxTelephonyEventPkt Integer32,
                   tbHostFirewallStatRtpTxTelephonyEventPkt Integer32,
                   tbHostFirewallStatRtpRxNsePkt            Integer32,
                   tbHostFirewallStatRtpTxNsePkt            Integer32,
                   tbHostFirewallStatRtpRxComfortNoisePkt   Integer32,
                   tbHostFirewallStatRtpTxComfortNoisePkt   Integer32,
                   tbHostFirewallStatRtpRxPayloadTypeChange Integer32,
                   tbHostFirewallStatRtpRxSsrcChange        Integer32,
                   tbHostFirewallStatRtpRxSeqError          Integer32,
                   tbHostFirewallStatRtpRxBadPkt            Integer32,
                   tbHostFirewallStatRtcpRxPkt              Integer32,
                   tbHostFirewallStatRtcpRxLostPkt          Integer32,
                   tbHostFirewallStatRtcpRxBadPkt           Integer32,
                   tbHostFirewallStatRtcpTxPkt              Integer32,
                   tbHostFirewallStatUdptlRxPkt             Integer32,
                   tbHostFirewallStatUdptlSeqError          Integer32,
                   tbHostFirewallStatUdptlBadPkt            Integer32,
                   tbHostFirewallStatUdptlTxPkt             Integer32
                 }

    tbHostFirewallName OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Name of this Host."
        ::= { tbHostFirewallEntry 1 }

    tbHostFirewallState OBJECT-TYPE
        SYNTAX  INTEGER {
                          invalid(0),
                          on(1),
                          offWithStats(2),
                          off(3)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Current state of the firewall on this host."
        ::= { tbHostFirewallEntry 2 }

    tbHostFirewallSafeMode OBJECT-TYPE
        SYNTAX  INTEGER {
                          false(0),
                          true(1)
                         }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "SBC is running in safe mode due to a failure to start normally. Packets are not filtered in this mode."
        ::= { tbHostFirewallEntry 3 }

    tbHostFirewallSchedulingProblem OBJECT-TYPE
        SYNTAX  INTEGER {
                          false(0),
                          true(1)
                         }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "SBC's packet processing cores has detected scheduling problems in the last 60 seconds. Please contact support team to fix your host/VM configuration."
        ::= { tbHostFirewallEntry 4 }

    tbHostFirewallDDOS OBJECT-TYPE
        SYNTAX  INTEGER {
                          false(0),
                          true(1)
                         }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Suspected DDOS attack: Accepted or dropped connections list is full. This could indicate a DDOS attack, or that size of accepted/dropped connections lists (ACL) must be increased."
        ::= { tbHostFirewallEntry 5 }

    tbHostFirewallThresholdPassed OBJECT-TYPE
