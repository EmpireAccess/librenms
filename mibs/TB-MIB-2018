        ::= { tbHostFeature 2 }

    tbHostFeatureEntry OBJECT-TYPE
        SYNTAX  TBHostFeatureEntry
        MAX-ACCESS  not-accessible
        STATUS  current
        DESCRIPTION
            "A Host feature table entry."
        INDEX   { tbModuleIndex }
        ::= { tbHostFeatureTable 1 }
        
    TBHostFeatureEntry ::=
        SEQUENCE {
                   tbHostFeatureType                 DisplayString,
                   tbHostFeatureExpired              INTEGER,
                   tbHostFeatureAboutToExpire        INTEGER,
                   tbHostFeatureExpireDate           DisplayString,
                   tbHostFeatureExpireTime           Integer32,
                   tbHostFeatureEffectiveFeatureCnt  Integer32
                 }
                 
    tbHostFeatureType OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Name of the application."
        ::= { tbHostFeatureEntry 1}   
                                  
    tbHostFeatureExpired OBJECT-TYPE
        SYNTAX  INTEGER {
                          false(0),
                          true(1)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "Expired"
        ::= { tbHostFeatureEntry 2}   
                                  
    tbHostFeatureAboutToExpire OBJECT-TYPE
        SYNTAX  INTEGER {
                          false(0),
                          true(1)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "About to expire"
        ::= { tbHostFeatureEntry 3}          
                                  
    tbHostFeatureExpireDate OBJECT-TYPE
        SYNTAX  DisplayString
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
