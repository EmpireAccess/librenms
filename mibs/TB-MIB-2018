        STATUS  current
        DESCRIPTION
            "Indication that host operating system update is available"
        ::= { tbHostEntry 3 }

     tbHostOamMode OBJECT-TYPE
        SYNTAX  INTEGER {
                          master(0),
                          slave(1)
                        }
        MAX-ACCESS  read-only
        STATUS  current
        DESCRIPTION
            "There's only 1 master in a toolpack system at a given time. Any hosts can become master if necessary. The master controls Toolpack, the database and various states."
        ::= { tbHostEntry 4 }

    tbHostRole OBJECT-TYPE
        SYNTAX  INTEGER {
                          unknown(0),
                          primary(1),
                          secondary(2),
                          standalone(3)
                        }
